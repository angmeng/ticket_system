<%= simple_form_for @routine, :html => { :class => 'form-vertical' } do |f| %>
  <%= f.input :code %>
  <%= f.association :departure_jetty %>
  <%= f.association :arrival_jetty %>
  <%= f.input :sequence_no %>
  <%= f.input :active %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                routines_path, :class => 'btn' %>
  </div>
<% end %>
