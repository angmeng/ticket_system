<%= simple_form_for @routine, :html => { :class => 'form-vertical' } do |f| %>
  <%= f.input :code %>
  <%= f.input :departure_jetty_id %>
  <%= f.input :arrival_jetty_id %>
  <%= f.input :sequence_no %>
  <%= f.input :active %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                routines_path, :class => 'btn' %>
  </div>
<% end %>
